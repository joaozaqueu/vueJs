<!doctype html>
<html>
    <head>
    <meta charset="utf-8">
    <title> Iniciando com Vue Js </title>

    <!-- Compiled and minified CSS and SCRIPT Materialize-CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>

    <link rel="stylesheet" href="css/style.css">
    <script src="https://cdn.jsdelivr.net/npm/vue"></script>
    </head>
    <body>
        <div id="vue-app">
            <h1>{{ greet('Teste funcional') }}</h1>
            <h3>{{message}} - {{job}}</h3>

            <a :href="site">Google</a>
            <p v-html="testSite"></p>

            <button class="btn waves-effect waves-light" v-on:click="add(5)">Add 5 Year</button>
            <button class="btn waves-effect waves-light" v-on:click="subtract(2)">Subtract 2 Year</button>

            <div class="card-panel">Age is {{age}}</div>
        </div>

    <script src="js/app.js"></script>
    </body>
</html>